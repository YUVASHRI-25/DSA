   "source": [
    "#ARMSTRONG NUMBER\n",
    "n = int(input())\n",
    "copy = n\n",
    "count = len(str(n))  \n",
    "sum = 0\n",
    "\n",
    "while n > 0:\n",
    "    digit = n % 10\n",
    "    sum += digit ** count\n",
    "    n //= 10\n",
    "\n",
    "if sum == copy:\n",
    "    print(\"Armstrong number\")\n",
    "else:\n",
    "    print(\"Not an Armstrong number\")\n"
   ]
  }
