   "source": [
    "#(Print N to 1 using recursion)\n",
    "def print_n_to_1(n):\n",
    "    if n == 0:   # base case\n",
    "        return\n",
    "    print(n)\n",
    "    print_n_to_1(n - 1)   # recursive call\n"
   ]
  }
